{
  "name": "inventory-app",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "devDependencies": {
    "electron": "^30.0.0",
    "electron-builder": "^24.0.0"
  },
  "build": {
    "appId": "com.bissell.inventory",
    "productName": "InventoryApp",
    "directories": {
    "output": "dist"
    },
    "files": [
    "**/*"
    ],
    "linux": {
      "target": ["AppImage", "deb"],
      "category": "Utility"
    },
      "armv7l": {
      "target": ["AppImage", "deb"]
    },
      "arm64": {
      "target": ["AppImage", "deb"]
    }
  }
}